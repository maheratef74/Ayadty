@{
    Layout = "_AdminLayout";
}

@inject IViewLocalizer localizer
@using System.Globalization
@using Microsoft.AspNetCore.Mvc.Localization


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@localizer["Add_Update_Delete"] </title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href=".css/UP_Add_Del.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


</head>

<Body>
    <hr>
    <div class="container bootstrap snippets bootdey">
        <div class="row">
            <div class="col-lg-12">
                <div class="main-box no-header clearfix">
                    <div class="main-box-body clearfix">
                        <div class="table-responsive">
                            <table class="table user-list">
                                <thead>
                                    <tr>
                                        <th><span>@localizer["ID"]</span></th>
                                        <th><span>@localizer["User"]</span></th>
                                        <th><span>@localizer["Date"]</span></th>
                                        <th><span>@localizer["Time"]</span></th>
                                        <th class="text-center"><span>@localizer["Status"]</span></th>
                                        <th><span>@localizer["Phone Number"]</span></th>


                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>@localizer["1"]</td>
                                        <td>
                                            <img src="https://bootdey.com/img/Content/user_1.jpg" alt="">
                                            <a href="#" class="user-link">@localizer["John Doe"]</a>

                                        </td>
                                        <td>@localizer["2024/08/12"]</td>
                                        <td>@localizer["12:18:09"]</td>
                                        <td class="text-center">
                                            <span class="label label-default">@localizer["pending"]</span>
                                        </td>
                                        <td>
                                            <a href="#">@localizer["+201021587031"]</a>
                                        </td>
                                        <td style="width: 20%;">
                                            <a class="table-link text-warning" target="__blank" href="Add.html">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
                                                </span>
                                                <a class="table-link text-info" target="__blank" href="Edit.html">
                                                    <span class="fa-stack">
                                                        <i class="fa fa-square fa-stack-2x"></i>
                                                        <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>

                                                    </span>
                                                </a>

                                                <a class="table-link danger" target="__blank" href="Delete.html">
                                                    <span class="fa-stack">
                                                        <i class="fa fa-square fa-stack-2x"></i>
                                                        <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                                    </span>
                                                </a>
                                            </a>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>@localizer["2"]</td>
                                        <td>
                                            <img src="https://bootdey.com/img/Content/user_3.jpg" alt="">
                                            <a href="#" class="user-link">@localizer["Full name 2"]</a>

                                        </td>
                                        <td>@localizer["2024/08/12"]</td>
                                        <td>@localizer["11:48:05"]</td>
                                        <td class="text-center">
                                            <span class="label label-success">@localizer["Active"]</span>
                                        </td>
                                        <td>
                                            <a href="#">@localizer["+201123463412"]</a>
                                        </td>
                                        <td style="width: 20%;">
                                            <a  class="table-link text-warning" target="__blank" href="Add.html">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
                                                </span>
                                                <a class="table-link text-info" target="__blank" href="Edit.html">
                                                    <span class="fa-stack">
                                                        <i class="fa fa-square fa-stack-2x"></i>
                                                        <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>

                                                    </span>
                                                </a>
                                                <a class="table-link danger" target="__blank" href="Delete.html">
                                                    <span class="fa-stack">
                                                        <i class="fa fa-square fa-stack-2x"></i>
                                                        <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                                    </span>
                                                </a>
                                            </a>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>@localizer["3"]</td>
                                        <td>
                                            <img src="https://bootdey.com/img/Content/user_2.jpg" alt="">
                                            <a href="#" class="user-link">@localizer["Full name 3"]</a>

                                        </td>
                                        <td>@localizer["2024/08/12"]</td>
                                        <td>@localizer["10:18:09"]</td>
                                        <td class="text-center">
                                            <span class="label label-danger">@localizer["inactive"]</span>
                                        </td>
                                        <td>
                                            <a href="#">@localizer["+201235801357"]</a>
                                        <td style="width: 20%;">
                                            <a class="table-link text-warning"target="__blank" href="Add.html">
                                                <span class="fa-stack">
                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                    <i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
                                                </span>
                                                <a class="table-link text-info" target="__blank" href="Edit.html">
                                                    <span class="fa-stack">
                                                        <i class="fa fa-square fa-stack-2x"></i>
                                                        <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>

                                                    </span>
                                                </a>
                                                <a class="table-link danger"target="__blank" href="Delete.html">
                                                    <span class="fa-stack">
                                                        <i class="fa fa-square fa-stack-2x"></i>
                                                        <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                                    </span>
                                                </a>
                                            </a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</Body>

</html>
